<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd">
<hibernate-reverse-engineering>
    <schema-selection match-catalog="gsst" match-schema="public"/>
    <table-filter match-name="funcionario"/>
    <table-filter match-name="valores"/>
    <table-filter match-name="processo"/>
    <table-filter match-name="unidade"/>
    <table-filter match-name="nr"/>
    <table-filter match-name="maquina"/>
    <table-filter match-name="usuario"/>
    <table-filter match-name="empresa"/>
    <table-filter match-name="processo_nr"/>
</hibernate-reverse-engineering>
